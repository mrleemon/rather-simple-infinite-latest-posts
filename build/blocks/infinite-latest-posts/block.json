{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "occ/rather-simple-infinite-latest-posts",
  "title": "Rather Simple Infinite Latest Posts",
  "description": "Display latest posts.",
  "textdomain": "rather-simple-infinite-latest-posts",
  "category": "widgets",
  "attributes": {
    "category": {
      "type": "integer",
      "default": 0
    },
    "number": {
      "type": "integer",
      "default": 5
    }
  },
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js",
  "style": "file:./style-index.css"
}